<section class="main-wrap">
  <div class="container">
    
    <div class="row">
      <% data.forEach(function(data){ %>
      <div class="col-md-4 p-5">
        <div class="card" style="width: 18rem;">
          <a href="/fullImage/<%= data._id %>">
           <img src="/images/<%= data.Image %>" class="card-img-top" style="height: 200px;" alt="...">
          </a>
          <div class="card-body">
            <h5 class="card-title"><%= data.ProductName %></h5>
            <p class="card-text"><%= data.Category %></p>
            <p class="card-text">Rs <%= data.Price %></p>

            <button onclick="addToCart('<%= data._id %>'); Swal.fire({
              position: 'center',
              icon: 'success',
              title: 'Item Added Successfully To Cart',
              showConfirmButton: false,
              timer: 2000
            })"   class="btn btn-primary">Add to Cart</button>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>
        

  
